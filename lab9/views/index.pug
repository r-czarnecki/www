html
  head
    title= title
  style
    include ../public/stylesheets/style.css
  body
    h1= message
    img(src="images/hello.jpg", id="hello")
    h1="Meme list"
    ul
      each meme in memes
        li
          a(href="meme/" + meme.id) #{meme.name}
          img(src=meme.url)
          p="Price: " + meme.price